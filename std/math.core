load(std "std/std.core")

product : func([l] {std.fold(l 1 {*})})

sum : func([l] {std.fold(l 0 {+} )}) 

sq : func([x] {x * x})

fib : func([num] {
    ptp : 0
    p   : 1
    sum : 0
    if( num = 0 {
        sum : 0
    } {
        if( num = 1 {
            sum : 1 
        } {
            for(i range(0 (num - 2)) {
                sum : ptp + p
                ptp : return(p)
                p   : return(sum)
            })
        })
    })
    return(sum)
})