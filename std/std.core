map : func([l q] {
    nl : []
    each(l {
        q()
        push(nl)
        set(nl)
    })
    return(nl)
})

fold : func([l s q] {return(s) each(l {q()}) })

filter : func([l q] {
    nl : []
    each(l {
        dup()
        q()
        if({
            push(nl)
            set(nl)
        } {
            drop()
        })
    })
    return(nl)
})

# combos
dip : func([x y] {y() return(x)})
keep : func([x y] {y(x) return(x)})
bi : func([p q x] {x(p) x(q)})